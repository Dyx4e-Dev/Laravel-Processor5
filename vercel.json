{
    "version": 2,
    "builds": [
        {
            "src": "api/index.php",
            "use": "@vercel/php"
        }
    ],
    "routes": [
        {
            "src": "/build/(.*)",
            "dest": "public/build/$1"
        },
        {
            "src": "/css/(.*)",
            "dest": "public/css/$1"
        },
        {
            "src": "/js/(.*)",
            "dest": "public/js/$1"
        },
        {
            "src": "/img/(.*)",
            "dest": "public/img/$1"
        },
        {
            "src": "/anim/(.*)",
            "dest": "public/anim/$1"
        },
        {
            "src": "/(favicon.ico|robots.txt)",
            "dest": "public/$1"
        },
        {
            "src": "/(.*)",
            "dest": "api/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "APP_URL": "https://laravel-processor5.vercel.app",
        "APP_KEY": "${APP_KEY}",
        "DB_CONNECTION": "${DB_CONNECTION}",
        "DB_HOST": "${DB_HOST}",
        "DB_PORT": "${DB_PORT}",
        "DB_DATABASE": "${DB_DATABASE}",
        "DB_USERNAME": "${DB_USERNAME}",
        "DB_PASSWORD": "${DB_PASSWORD}",
        "APP_CONFIG_CACHE": "/tmp/config.php",
        "VIEW_COMPILED_PATH": "/tmp/views",
        "CACHE_DRIVER": "array",
        "LOG_CHANNEL": "stderr",
        "SESSION_DRIVER": "cookie"
    }
}
